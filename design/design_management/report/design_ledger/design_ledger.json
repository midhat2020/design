 "prepared_report": 0,
 "query": "select\n    a.`posting_date` as `Date`,\n    a.`posting_time` as `Time`,\n    a.`item_code` as `Item`,\n    (case\n    when a.`qty_change` > 0 then CONCAT('<p style=\"color:green;\">', ROUND(a.`qty_change`,2), '</p><br />')\n    else 0.00 end) as `In Qty`,\n    (case\n    when a.`qty_change` < 0 then CONCAT('<p style=\"color:red;\">', ROUND(a.`qty_change`,2), '</p><br />')\n    else 0.00 end) as `Out Qty`,\n    CONCAT('<p style=\"color:black;\">', ROUND(a.`qty_after_transaction`,2), '</p><br />')as `Balance Qty`,\n    a.`department` as `Department`,\n    a.`voucher_no` as `Voucher #`,\n    a.`voucher_type` as `Voucher Type`\nfrom `tabDesign Ledger Entry` a\nwhere a.`voucher_no`= %(voucher_no)s and a.`is_cancelled` = 0\norder by a.`posting_date`,a.`posting_time` asc",
